                                                        <a:da name="DEFAULT" type="XPath" expr="num:i(node:fallback(node:current()/../@index, num:i(0)))"/>
                                                        <a:da name="RANGE" type="XPath">
                                                            <a:tst expr="text:uniq(../../*/PmicAmuxChannelId, .)"
                                                                false="Duplicated value: PmicAmuxChannelId must be unique across all AMUX channels. Use the 'Calculate  Value' button to calculate the correct default value."/>
                                                            <a:tst expr="(. &gt;= 0) and (. &lt; num:i(count(node:current()/../../*)))"
                                                                false="Value out of range: must be in range 0 to N-1 (N is number of configured AMUX channels). Use the 'Calculate Value' button to calculate the correct default value."/>
                                                            <a:tst expr="num:i(node:fallback(.,0)) = num:i(../@index)"
                                                                false="Invalid value: The ID should correspond to the position of the element within the configuration list "/>
                                                        </a:da>
                                                        <a:da name="INVALID" type="Range">
                                                            <a:tst expr="&lt;=255"/>
                                                            <a:tst expr="&gt;=0"/>
                                                        </a:da>
                                                    </v:var>

                                                    <v:var name="PmicAmuxChannelSelect" type="ENUMERATION">
                                                        <a:a name="DESC">
                                                            <a:v>
                                                                <![CDATA[EN:<html>
                                                                    Select the AMUX channel for the specific instance of the PmicAmuxChannel container (M_AMUX[AMUX]).<p/>
                                                                    Note: Implementation Specific Parameter. <p/>
                                                                </html>]]>
                                                            </a:v>
                                                        </a:a>
                                                        <a:a name="LABEL" value="AMUX Channel Select" />
                                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                        </a:a>
                                                        <a:a name="ORIGIN" value="NXP"/>
                                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                        <a:a name="SCOPE" value="LOCAL"/>
                                                        <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                                        <a:a name="UUID" value="ECUC:e699f695-3db5-4f60-a174-5ac4723a8247f"/>
                                                        <a:da name="DEFAULT" type="XPath" expr="ecu:list('PMIC.AMUX.Channels.List')[node:fallback(node:current()/../@index, num:i(0)) + num:i(1)]"/>
                                                        <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.AMUX.Channels.List')"/>
                                                    </v:var>

                                                    <v:var name="PmicAmuxRatio" type="ENUMERATION">
                                                      <a:a name="DESC">
                                                          <a:v>
                                                              <![CDATA[EN:
                                                                  <html>
                                                                  Controls the divider ratio for VSUP, PWRON1, and PWRON2 (M_AMUX[RATIO]). <p/>
                                                                  This field is editable only when PmicAmuxChannelSelect is either VSUP, PWRON1 or PWRON2. <p/>
                                                                  <strong>Warning:</strong>  The input voltage range for VSUP, PWRON1, and PWRON2 must be
                                                                  between 2.25V and 22.5V when RATIO = 20, and between 4.2V and 42V when RATIO = 34. <p/>
                                                                  Note: Implementation Specific Parameter. <p/>
                                                                  </html>
                                                              ]]>
                                                          </a:v>
                                                      </a:a>
                                                      <a:a name="LABEL" value="AMUX Ratio" />
                                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                      </a:a>
                                                      <a:a name="ORIGIN" value="NXP"/>
                                                      <a:a name="SCOPE" value="LOCAL"/>
                                                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                      <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-a5d3e247da5f"/>
                                                      <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.AMUX.Ratio.Default')"/>
                                                      <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.AMUX.Ratio.List')"/>
                                                      <a:a name="EDITABLE" type="XPath">
                                                          <a:tst expr="(node:fallback(../PmicAmuxChannelSelect, ecu:get('PMIC.AMUX.Channels.Default')) = 'VSUP1')   or
                                                                       (node:fallback(../PmicAmuxChannelSelect, ecu:get('PMIC.AMUX.Channels.Default')) = 'PWRON1') or
                                                                       (node:fallback(../PmicAmuxChannelSelect, ecu:get('PMIC.AMUX.Channels.Default')) = 'PWRON2')"/>
                                                      </a:a>
                                                    </v:var>
                                                </v:ctr>
                                            </v:lst>

                                        </v:ctr>

                                        <!-- /** @implements PmicFailSafeConfiguration_Object */ -->
                                        <v:ctr name="PmicFailSafeConfiguration" type="IDENTIFIABLE">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        This container contains the initial configuration parameters (set in the INIT_FS state) for the overvoltage and undervoltage reactions. <p/>
                                                        Note: Implementation Specific Parameter. <p/>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="TAB" value="PmicFailSafeConfiguration"/>
                                            <a:a name="UUID" value="ECUC:cd105304-074e-4397-8451-5702fd1c38fc"/>

                                            <v:ctr name="PmicSVSConfiguration" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:
                                                            <html>
                                                            This container contains the initial configuration parameters (set in the INIT_FS state) for the SVS mechanism. <p/>
                                                            Note: Implementation Specific Parameter. <p/>
                                                            </html>
                                                        ]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="UUID" value="ECUC:9f1209fb-5482-4e6b-bf29-731e2d56ac2d"/>

                                                <v:var name="PmicSwitchSVSApi" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Pre-processor allows to switch static voltage scaler value. <p/>
                                                                User need to configure SVS setting in tab PmicSVSSettingConf. <p/>
                                                                In case DIE_PROCESS feature is enable, this function will configure SVS if the DIE_PROCESS otp of mcu enables. <p/>
                                                                It will not configure SVS if the DIE_PROCESS otp of mcu disable. Refer Mcu reference manual to check DIE_PROCESS bit. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="Switch SVS API Enable"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:7ea93c38-abcd-4571-a374-e3b14a6746b1"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                    <a:a name="INVALID" type="XPath">
                                                        <a:tst expr="
                                                            (
                                                                ( node:fallback(., 'false') = 'true' ) and
                                                                ( node:fallback(/AUTOSAR/TOP-LEVEL-PACKAGES/Pmic/ELEMENTS/Pmic/PmicGeneralConfiguration/PmicSwitchSVSApi, 'false') = 'false' )
                                                            )"
                                                            true="The field PmicGeneralConfiguration/PmicSwitchSVSApi must be set to 'true' when the switch SVS API is enabled for at least one device."/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="PmicSVSOffset" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                Statically scales the output voltage of BUCK1 (and BUCK2 if used in dual-phase mode)
                                                                by the configured amount in the direction specified by SVS_OFFSET_SIGN
                                                                in the INIT_FS state (FS_I_SVS[SVS_OFFSET]). <p/>
                                                                A SVS (Static Voltage Scaling) mechanism is implemented to allow the MCU to reduce
                                                                the output voltage initially configured at start-up of BUCK1 (and BUCK2 if
                                                                used in multiphase) by OTP, to optimize MCU voltage working points, and
                                                                compensate part to part process variation. <p/>
                                                                The offset amount is effectively limited by the allowed SVS steps configured
                                                                in VCORE_SVS_CLAMP_OTP[5:0] by OTP (i.e. PmicFailSafeConfiguration/PmicOtpSafetyConfiguration/
                                                                PmicSVSLimit) <p/>
                                                                Depending on the offset, it may be needed to change the voltage in multiple steps
                                                                to not trigger an OV/UV event. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                                </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="LABEL" value="SVS Offset (mV)" />
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-d2e3ce21eb7a"/>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.SVS.VoltageOffset.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.SVS.VoltageOffset.List')"/>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="(node:fallback(., ecu:get('PMIC.SVS.VoltageOffset.Default'))) != ecu:get('PMIC.SVS.VoltageOffset.Default') and
                                                                     (
                                                                        num:i( num:f(text:replace(text:replace(node:fallback(., ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.')) div num:f(6.25) + num:f(1) ) &gt;
                                                                        num:i( text:replace(node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyConfiguration/PmicSVSLimit, '0'),'MAX_STEP_','') )
                                                                     )"
                                                               true="The offset amount is greater than the maximum number of allowed steps configured by OTP (PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyConfiguration/PmicSVSLimit)."/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="PmicSVSOffsetSign" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                Configures the offset type applicable by the SVS mechanism (FS_I_SVS[SVS_OFFSET_SIGN]). <p/>
                                                                The offset type is limited by the SVS offset type configured by OTP in VCORE_SVS_FULL_OFFSET_OTP
                                                                (i.e. PmicFailSafeConfiguration/PmicOtpSafetyConfiguration/PmicSVSOffsetType) <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                                </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="LABEL" value="SVS Offset Sign" />
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-a1e3ec12eb7a"/>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.SVS.OffsetSign.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.SVS.OffsetSign.List')"/>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="(node:fallback(., ecu:get('PMIC.SVS.OffsetSign.Default')) = 'POSITIVE_OFFSET') and
                                                                     (node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyConfiguration/PmicSVSOffsetType, 'NEGATIVE_OFFSET') = 'NEGATIVE_OFFSET')"
                                                               true="The SVS offset sign is configured by OTP to support only negative offsets (i.e. PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyConfiguration/PmicSVSOffsetType = 'NEGATIVE_OFFSET')"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="PmicBuck1SVSOutputVoltage" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                Represents the output voltage of the BUCK1 (BUCK1/2 in dual-phase mode) regulator
                                                                after the SVS offset is applied. This output voltage will take effect at the
                                                                end of the INIT_FS state. <p/>
                                                                The formula for calculating this field is: <p/>
                                                                1. If PmicSVSOffsetSign = NEGATIVE_OFFSET: <p/>
                                                                PmicBuck1SVSOutputVoltage = PmicOtpMainUnitConfiguration/PmicOtpRegulatorsConfiguration/PmicBuck1Regulator/PmicBuck1OutputVoltage - PmicSVSOffset<p/>
                                                                2. If PmicSVSOffsetSign = POSITIVE_OFFSET: <p/>
                                                                PmicBuck1SVSOutputVoltage = PmicOtpMainUnitConfiguration/PmicOtpRegulatorsConfiguration/PmicBuck1Regulator/PmicBuck1OutputVoltage + PmicSVSOffset<p/>
                                                                Note: This field must not be manually modified.<p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                                </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="LABEL" value="BUCK1 SVS Output Voltage (V)" />
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                      <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-a1e7c5c2bd3f"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:f(
                                                                        num:f(
                                                                            num:f(num:f(node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpRegulatorsConfiguration/PmicBuck1Regulator/PmicBuck1OutputVoltage, '0.4')) * num:f(1000)) +
                                                                            (
                                                                                num:f(number( node:fallback(../PmicSVSOffsetSign, ecu:get('PMIC.SVS.OffsetSign.Default')) = 'POSITIVE_OFFSET' )) *
                                                                                num:f(text:replace(text:replace(node:fallback(../PmicSVSOffset, ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.'))
                                                                            ) -
                                                                            (
                                                                                num:f(number( node:fallback(../PmicSVSOffsetSign, ecu:get('PMIC.SVS.OffsetSign.Default')) = 'NEGATIVE_OFFSET' )) *
                                                                                num:f(text:replace(text:replace(node:fallback(../PmicSVSOffset, ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.'))
                                                                            )
                                                                        ) div num:f(1000)
                                                                    )" />
                                                    </a:da>
                                                    <a:da name="INVALID" type="Range">
                                                      <a:tst expr="&gt;=0.4"/>
                                                      <a:tst expr="&lt;=1.8"/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                      <a:tst expr="num:f(node:fallback(., 0.4)) !=
                                                                   num:f(
                                                                        num:f(
                                                                            num:f(num:f(node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpRegulatorsConfiguration/PmicBuck1Regulator/PmicBuck1OutputVoltage, '0.4')) * num:f(1000)) +
                                                                            (
                                                                                num:f(number( node:fallback(../PmicSVSOffsetSign, ecu:get('PMIC.SVS.OffsetSign.Default')) = 'POSITIVE_OFFSET' )) *
                                                                                num:f(text:replace(text:replace(node:fallback(../PmicSVSOffset, ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.'))
                                                                            ) -
                                                                            (
                                                                                num:f(number( node:fallback(../PmicSVSOffsetSign, ecu:get('PMIC.SVS.OffsetSign.Default')) = 'NEGATIVE_OFFSET' )) *
                                                                                num:f(text:replace(text:replace(node:fallback(../PmicSVSOffset, ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.'))
                                                                            )
                                                                        ) div num:f(1000)
                                                                    )"
                                                             true="Please press the button to calculate the value automatically."/>
                                                    </a:da>
                                                </v:var>

                                                <v:lst name="PmicSVSSettingConf" type="MAP">
                                                    <a:da name="MIN" type="XPath" expr="num:i(number( node:fallback(../PmicSwitchSVSApi, 'false') = 'true' ))"/>
                                                    <a:da name="INVALID" type="XPath">
                                                            <a:tst expr="(node:fallback(../PmicSwitchSVSApi, 'true') = 'false') and
                                                                         (count(./*) &gt;= 1)"
                                                                   true="This list must be empty unless the Switch SVS API is enabled (PmicFailSafeConfiguration/PmicSVSConfiguration/PmicSwitchSVSApi = 'true')"/>
                                                        </a:da>
                                                    <v:ctr name="PmicSVSSettingConf" type="IDENTIFIABLE">
                                                        <a:a name="REQUIRES-INDEX" value="true"/>
                                                        <a:a name="DESC">
                                                            <a:v>
                                                                <![CDATA[EN:<html>
                                                                    This container contains the configuration for the static voltage scaler settings of the PMIC. <p/>
                                                                    If the PmicSwitchSVSApi is enabled, the list must have at least one element.
                                                                    Otherwise, the list must be empty. <p/>
                                                                    Note: Implementation Specific Parameter. <p/>
                                                                </html>]]>
                                                            </a:v>
                                                        </a:a>
                                                        <a:a name="UUID" value="ECUC:dcc84cdf-93ef-4cca-821b-e13fd54aa123"/>
                                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                            <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                                            <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                                        </a:a>

                                                        <v:var name="PmicSVSSettingId" type="INTEGER">
                                                            <a:a name="DESC">
                                                                <a:v>
                                                                    <![CDATA[EN:<html>
                                                                        This parameter shall represent the ID of the SVS configuration. <p/>
                                                                        Note: Implementation Specific Parameter. <p/>
                                                                    </html>]]>
                                                                </a:v>
                                                            </a:a>
                                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                            </a:a>
                                                            <a:a name="LABEL" value="SVS Setting ID"/>
                                                            <a:a name="ORIGIN" value="NXP"/>
                                                            <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                                            <a:a name="SCOPE" value="LOCAL"/>
                                                            <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                                            <a:a name="UUID" value="ECUC:f296ee65-d7f7-4f6d-ae0c-93b4e3dad123"/>
                                                            <a:da name="DEFAULT" type="XPath" expr="num:i(node:fallback(node:current()/../@index, num:i(0)))"/>
                                                            <a:da name="RANGE" type="XPath">
                                                                <a:tst expr="text:uniq(../../*/PmicSVSSettingId, .)"
                                                                    false="Duplicated value: PmicSVSSettingId must be unique across all SVS configurations. Use the 'Calculate  Value' button to calculate the correct default value."/>
                                                                <a:tst expr="(. &gt;= 0) and (. &lt; num:i(count(node:current()/../../*)))"
                                                                    false="Value out of range: must be in range 0 to N-1 (N is number of configured SVS settings). Use the 'Calculate Value' button to calculate the correct default value."/>
                                                                <a:tst expr="num:i(node:fallback(.,0)) = num:i(../@index)"
                                                                    false="Invalid value: The ID should correspond to the position of the element within the configuration list "/>
                                                            </a:da>
                                                            <a:da name="INVALID" type="Range">
                                                                <a:tst expr="&lt;=255"/>
                                                                <a:tst expr="&gt;=0"/>
                                                            </a:da>
                                                        </v:var>

                                                        <v:var name="PmicSVSOffsetSetting" type="ENUMERATION">
                                                            <a:a name="DESC">
                                                                <a:v>
                                                                    <![CDATA[EN:
                                                                        <html>
                                                                        Statically scales the output voltage of BUCK1 (and BUCK2 if used in dual-phase mode)
                                                                        by the configured amount in the direction specified by SVS_OFFSET_SIGN
                                                                        in the INIT_FS state (FS_I_SVS[SVS_OFFSET]). <p/>
                                                                        A SVS (Static Voltage Scaling) mechanism is implemented to allow the MCU to reduce
                                                                        the output voltage initially configured at start-up of BUCK1 (and BUCK2 if
                                                                        used in multiphase) by OTP, to optimize MCU voltage working points, and
                                                                        compensate part to part process variation. <p/>
                                                                        The offset amount is effectively limited by the allowed SVS steps configured
                                                                        in VCORE_SVS_CLAMP_OTP[5:0] by OTP (i.e. PmicFailSafeConfiguration/PmicOtpSafetyConfiguration/
                                                                        PmicSVSLimit) <p/>
                                                                        Depending on the offset, it may be needed to change the voltage in multiple steps
                                                                        to not trigger an OV/UV event. <p/>
                                                                        Note: Implementation Specific Parameter. <p/>
                                                                        </html>
                                                                    ]]>
                                                                </a:v>
                                                            </a:a>
                                                            <a:a name="LABEL" value="SVS Offset (mV)" />
                                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                            </a:a>
                                                            <a:a name="ORIGIN" value="NXP"/>
                                                            <a:a name="SCOPE" value="LOCAL"/>
                                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                            <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-d2e3cdefeb7a"/>
                                                            <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.SVS.VoltageOffset.Default')"/>
                                                            <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.SVS.VoltageOffset.List')"/>
                                                            <a:da name="INVALID" type="XPath">
                                                                <a:tst expr="(node:fallback(., ecu:get('PMIC.SVS.VoltageOffset.Default'))) != ecu:get('PMIC.SVS.VoltageOffset.Default') and
                                                                             (
                                                                                num:i( num:f(text:replace(text:replace(node:fallback(., ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.')) div num:f(6.25) + num:f(1) ) &gt;
                                                                                num:i( text:replace(node:fallback(../../../../../PmicOtpConfiguration/*[1]/PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyConfiguration/PmicSVSLimit, '0'),'MAX_STEP_','') )
                                                                             )"
                                                                       true="The offset amount is greater than the maximum number of allowed steps configured by OTP (PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyConfiguration/PmicSVSLimit)."/>
                                                            </a:da>
                                                        </v:var>

                                                        <v:var name="PmicSVSOffsetSignSetting" type="ENUMERATION">
                                                            <a:a name="DESC">
                                                                <a:v>
                                                                    <![CDATA[EN:
                                                                        <html>
                                                                        Configures the offset type applicable by the SVS mechanism (FS_I_SVS[SVS_OFFSET_SIGN]). <p/>
                                                                        The offset type is limited by the SVS offset type configured by OTP in VCORE_SVS_FULL_OFFSET_OTP
                                                                        (i.e. PmicFailSafeConfiguration/PmicOtpSafetyConfiguration/PmicSVSOffsetType) <p/>
                                                                        Note: Implementation Specific Parameter. <p/>
                                                                        </html>
                                                                    ]]>
                                                                </a:v>
                                                            </a:a>
                                                            <a:a name="LABEL" value="SVS Offset Sign" />
                                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                            </a:a>
                                                            <a:a name="ORIGIN" value="NXP"/>
                                                            <a:a name="SCOPE" value="LOCAL"/>
                                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                            <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-a1efff12eb7a"/>
                                                            <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.SVS.OffsetSign.Default')"/>
                                                            <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.SVS.OffsetSign.List')"/>
                                                            <a:da name="INVALID" type="XPath">
                                                                <a:tst expr="(node:fallback(., ecu:get('PMIC.SVS.OffsetSign.Default')) = 'POSITIVE_OFFSET') and
                                                                             (node:fallback(../../../../../PmicOtpConfiguration/*[1]/PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyConfiguration/PmicSVSOffsetType, 'NEGATIVE_OFFSET') = 'NEGATIVE_OFFSET')"
                                                                       true="The SVS offset sign is configured by OTP to support only negative offsets (i.e. PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyConfiguration/PmicSVSOffsetType = 'NEGATIVE_OFFSET')"/>
                                                            </a:da>
                                                        </v:var>

                                                        <v:var name="PmicBuck1SVSOutputVoltageSetting" type="FLOAT">
                                                            <a:a name="DESC">
                                                                <a:v>
                                                                    <![CDATA[EN:
                                                                        <html>
                                                                        Represents the output voltage of the BUCK1 (BUCK1/2 in dual-phase mode) regulator
                                                                        after the SVS offset is applied. This output voltage will take effect at the
                                                                        end of the INIT_FS state. <p/>
                                                                        The formula for calculating this field is: <p/>
                                                                        1. If PmicSVSOffsetSign = NEGATIVE_OFFSET: <p/>
                                                                        PmicBuck1SVSOutputVoltage = PmicOtpMainUnitConfiguration/PmicOtpRegulatorsConfiguration/PmicBuck1Regulator/PmicBuck1OutputVoltage - PmicSVSOffset<p/>
                                                                        2. If PmicSVSOffsetSign = POSITIVE_OFFSET: <p/>
                                                                        PmicBuck1SVSOutputVoltage = PmicOtpMainUnitConfiguration/PmicOtpRegulatorsConfiguration/PmicBuck1Regulator/PmicBuck1OutputVoltage + PmicSVSOffset<p/>
                                                                        Note: This field must not be manually modified.<p/>
                                                                        Note: Implementation Specific Parameter. <p/>
                                                                        </html>
                                                                    ]]>
                                                                </a:v>
                                                            </a:a>
                                                            <a:a name="LABEL" value="BUCK1 SVS Output Voltage (V)" />
                                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                              <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                            </a:a>
                                                            <a:a name="ORIGIN" value="NXP"/>
                                                            <a:a name="SCOPE" value="LOCAL"/>
                                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                            <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-a1e7c5c2bfff"/>
                                                            <a:da name="DEFAULT" type="XPath">
                                                                <a:tst expr="num:f(
                                                                                num:f(
                                                                                    num:f(num:f(node:fallback(../../../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpRegulatorsConfiguration/PmicBuck1Regulator/PmicBuck1OutputVoltage, '0.4')) * num:f(1000)) +
                                                                                    (
                                                                                        num:f(number( node:fallback(../PmicSVSOffsetSignSetting, ecu:get('PMIC.SVS.OffsetSign.Default')) = 'POSITIVE_OFFSET' )) *
                                                                                        num:f(text:replace(text:replace(node:fallback(../PmicSVSOffsetSetting, ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.'))
                                                                                    ) -
                                                                                    (
                                                                                        num:f(number( node:fallback(../PmicSVSOffsetSignSetting, ecu:get('PMIC.SVS.OffsetSign.Default')) = 'NEGATIVE_OFFSET' )) *
                                                                                        num:f(text:replace(text:replace(node:fallback(../PmicSVSOffsetSetting, ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.'))
                                                                                    )
                                                                                ) div num:f(1000)
                                                                            )" />
                                                            </a:da>
                                                            <a:da name="INVALID" type="Range">
                                                              <a:tst expr="&gt;=0.4"/>
                                                              <a:tst expr="&lt;=1.8"/>
                                                            </a:da>
                                                            <a:da name="INVALID" type="XPath">
                                                              <a:tst expr="num:f(node:fallback(., 0.4)) !=
                                                                           num:f(
                                                                                num:f(
                                                                                    num:f(num:f(node:fallback(../../../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpRegulatorsConfiguration/PmicBuck1Regulator/PmicBuck1OutputVoltage, '0.4')) * num:f(1000)) +
                                                                                    (
                                                                                        num:f(number( node:fallback(../PmicSVSOffsetSignSetting, ecu:get('PMIC.SVS.OffsetSign.Default')) = 'POSITIVE_OFFSET' )) *
                                                                                        num:f(text:replace(text:replace(node:fallback(../PmicSVSOffsetSetting, ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.'))
                                                                                    ) -
                                                                                    (
                                                                                        num:f(number( node:fallback(../PmicSVSOffsetSignSetting, ecu:get('PMIC.SVS.OffsetSign.Default')) = 'NEGATIVE_OFFSET' )) *
                                                                                        num:f(text:replace(text:replace(node:fallback(../PmicSVSOffsetSetting, ecu:get('PMIC.SVS.VoltageOffset.Default')),'OFFSET_',''),'_','.'))
                                                                                    )
                                                                                ) div num:f(1000)
                                                                            )"
                                                                     true="Please press the button to calculate the value automatically."/>
                                                            </a:da>
                                                        </v:var>
                                                    </v:ctr>
                                                </v:lst>
