                                    </a:a>
                                    <a:a name="EDITABLE" type="XPath">
                                        <a:tst expr="(node:fallback(../PmicDieProcessEnable, 'true') = 'true')"/>
                                    </a:a>
                                    <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Ocotp/OcotpChannelList/OcotpChannel"/>
                                </v:ref>

