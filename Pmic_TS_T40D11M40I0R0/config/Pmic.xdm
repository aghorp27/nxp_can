        <d:lst type="TOP-LEVEL-PACKAGES">
            <!-- /** @implements Pmic_Object */ -->
            <d:ctr name="TS_T40D11M40I0R0" type="AR-PACKAGE">
                <a:a name="UUID" value="ECUC:5fe16860-10a9-6969-ba32-129de4bf6ac3"/>
                <d:lst type="ELEMENTS">
                    <d:chc name="Pmic" type="AR-ELEMENT" value="MODULE-DEF">
                        <v:ctr type="MODULE-DEF">
                            <a:a name="DESC">
                                <a:v>
                                    <![CDATA[EN:<html>
                                        Configuration of the Power Management Integrated Circuit (PMIC) module.
                                    </html>]]>
