                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the output voltage of the BUCK1 regulator (BUCK1V_OTP[7:0]). <p/>
                                                                          BUCK1 (BUCK1/2 in dual-phase mode) is intended to supply the MCU core voltage. <p/>
                                                                          The output voltage must have a resolution of 6.25 mV. <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="BUCK1 Output Voltage (V)" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-d1a5b7c2ad3a"/>
                                                              <a:da name="DEFAULT" value="0.8"/>
                                                              <a:da name="INVALID" type="Range">
                                                                <a:tst expr="&gt;=0.4"/>
                                                                <a:tst expr="&lt;=1.8"/>
                                                              </a:da>
                                                              <a:da name="INVALID" type="XPath">
                                                                <a:tst expr="( (num:f(num:f(node:fallback(., 0.4)) * num:f(100000)) div (num:f(0.00625) * num:f(100000)) ) != ( num:i((num:f(node:fallback(., 0.4)) * num:f(100000)) div (num:f(0.00625) * num:f(100000))) ) )"
                                                                       true="The output voltage must have a resolution of 6.25 mV (i.e. must be set in steps of 6.25 mV)"/>
                                                              </a:da>
                                                            </v:var>

                                                            <v:var name="PmicBuck1CurrentLimit" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls (limits) the output current of the BUCK1 regulator (BUCK1_ILIM_OTP[1:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
