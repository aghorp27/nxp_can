                                                              <a:a name="EDITABLE" type="XPath">
                                                                  <a:tst expr="node:fallback(../../PmicBuck1Regulator/PmicBuck1PhaseMode, ecu:get('PMIC.OTP.BUCK1.PhaseMode.Default')) = 'SINGLE_PHASE_MODE'"/>
