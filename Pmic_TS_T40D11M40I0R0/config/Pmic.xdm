                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.FCCU.Fccu12FaultPolarity.List')"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyIOConfiguration/PmicFccuEnable, 'true')"/>
                                                        <a:tst expr="node:fallback(../PmicFccuMonitoringConfiguration, ecu:get('PMIC.FCCU.MonitoringConfiguration.Default')) = 'FCCU1_AND_FCCU2_PAIR'"/>
