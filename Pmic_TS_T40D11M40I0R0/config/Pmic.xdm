                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Internal Oscillator Frequency [Hz]"/>
                                                    <a:a name="UUID" value="ECUC:cb7e14da-da73-4b07-9487-c8d37d4a5baf"/>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.IRCOSC.Frequencies.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.IRCOSC.Frequencies.List')"/>
                                                </v:var>

                                                <v:var name="PmicLowPowerOscFrequencyHz" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Low Power Oscillator Frequency [Hz] (LOW_POWER_CLOCK[1:0]). <p/>
                                                                This oscillator is used only in the low power standby mode
                                                                and has an accuracy of +/-10%. <p/>
                                                                The main purpose of this oscillator is to reduce the current
                                                                consumption of the device during the standby mode. <p/>
                                                                It is recommended to use the 600KHz setting for DDR self-refresh. <p/>
                                                                <strong>Warning:</strong> A frequency change of LPOSC needs
                                                                to be programmed at least 40us prior to transitioning the
                                                                VR5510 device into Standby Mode. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Low Power Oscillator Frequency [Hz]"/>
                                                    <a:a name="UUID" value="ECUC:cb7e14da-da73-4b07-9487-a3e73d6a5baf"/>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.LPOSC.Frequencies.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.LPOSC.Frequencies.List')"/>
                                                </v:var>

                                                <v:var name="PmicFinEnable" type="BOOLEAN">
                                                  <a:a name="DESC">
                                                      <a:v>
                                                          <![CDATA[EN:
                                                              <html>
                                                              This field is used to enable/disable the FIN pin (EXT_FIN_DIS) . <p/>
                                                                0 - FIN Pin is Disabled <p/>
                                                                1 - FIN Pin is Enabled <p/>
                                                              The PLL shall also be enabled by OTP in order to synchronize the switching regulators. <p/>
                                                              Note: Implementation Specific Parameter. <p/>
                                                              </html>
                                                          ]]>
                                                      </a:v>
                                                  </a:a>
                                                  <a:a name="LABEL" value="FIN Pin Enable" />
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-a2d3fe751d3b"/>
                                                  <a:da name="DEFAULT" value="false"/>
                                                  <a:da name="INVALID" type="XPath">
                                                      <a:tst expr="(
                                                                      ( node:fallback(., 'false') = 'true' ) and
                                                                      ( node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicPllEnable, 'false') = 'false' )
                                                                   )"
                                                             true="The PLL must also be enabled when enabling the FIN pin (i.e. PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicPllEnable = 'true')."/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="PmicExternalPIN_Fin_FrequencyHz" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                FIN Reference Frequency [Hz] (CLK_FIN_DIV). <p/>
                                                                The input frequency range of FIN must be between 333KHz-500kHz or 2MHz-3MHz. <p/>
                                                                If the external frequency coming from the FIN pin at runtime is out of range,
                                                                CLK moves back to the internal oscillator and an error is reported through
                                                                the CLK_FIN_DIV_OK bit. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="FIN External Reference Frequency [Hz]"/>
                                                    <a:a name="UUID" value="ECUC:72954dc6-f1aa-4644-92ca-b5d7384489ea"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="node:fallback(../PmicFinEnable, 'false') = 'true'"/>
                                                    </a:a>
                                                    <a:da name="DEFAULT" value="333000"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=3000000"/>
                                                        <a:tst expr="&gt;=333000"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="(
                                                                        ( num:f(node:fallback(., 0)) &lt; num:f(333000) ) or
                                                                        ( ( num:f(node:fallback(., 0)) &gt; num:f(500000) ) and ( num:f(node:fallback(., 0)) &lt; num:f(2000000) ) ) or
                                                                        ( num:f(node:fallback(., 0)) &gt; num:f(3000000) )
                                                                     )"
                                                               true="The input frequency range of FIN must be between 333KHz-500kHz or 2MHz-3MHz."/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="PmicPllClockSelection" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Configures the PLL Source Selection (EXT_FIN_SEL). <p/>
                                                                <strong>Warning:</strong> The PLL is enabled/disabled via the OTP settings. <p/>
                                                                This field is not editable unless the PLL is enabled
                                                                (PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicPllEnable = 'true'). <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="PLL Source Selection"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:9e8a476c-ef94-48e7-a9fa-cdb621badb58"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicPllEnable, 'false') = 'true'"/>
                                                    </a:a>
                                                    <a:da name="DEFAULT" value="IRCOSC_CLK"/>
                                                    <a:da name="RANGE">
                                                        <a:v>IRCOSC_CLK</a:v>
                                                        <a:v>FIN_CLK</a:v>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="( (node:fallback(., 'IRCOSC_CLK') = 'FIN_CLK') and (node:fallback(../PmicFinEnable, 'false') = 'false') )"
                                                               true="The FIN pin must be enabled in order to use it as an input source to the PLL (i.e PmicFinEnable = 'true')"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="PmicSpectrumModulationEnable" type="BOOLEAN">
                                                  <a:a name="DESC">
                                                      <a:v>
                                                          <![CDATA[EN:
                                                              <html>
                                                              This field is used to enable/disable the spread spectrum modulation feature (MOD_EN) . <p/>
                                                                0 - Spread Spectrum Modulation is Disabled <p/>
                                                                1 - Spread Spectrum Modulation is Enabled <p/>
                                                              The main purpose of the spread spectrum modulation is to improve the EMC performance
                                                              by spreading the energy of the internal oscillator and VPRE frequency on VBAT
                                                              frequency spectrum. <p/>
                                                              Note: Implementation Specific Parameter. <p/>
                                                              </html>
                                                          ]]>
                                                      </a:v>
                                                  </a:a>
                                                  <a:a name="LABEL" value="Spread Spectrum Modulation Enable" />
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-a5e1fc751d3b"/>
                                                  <a:da name="DEFAULT" value="false"/>
                                                  <a:da name="INVALID" type="XPath">
                                                      <a:tst expr="( (node:fallback(., 'false') = 'true') and (node:fallback(../PmicIrcoscFrequencyHz, 'F_20000000') != 'F_20000000') )"
                                                             true="Spectrum modulation cannot be enabled when manually tunning the frequency of IRCOSC (i.e PmicIrcoscFrequencyHz != 20 MHz)"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="PmicTriangularCarrierFrequencyHz" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Triangular Carrier Frequency [Hz] (MOD_CONF). <p/>
                                                                The spread spectrum modulation has a +/-5% deviation range around
                                                                the center of the oscillator frequency. <p/>
                                                                It is recommended to select 23.15KHz carrier frequency when VPRE is
                                                                configured at 455KHz and 92.6KHz when VPRE is configured at 2.22MHz
                                                                for best performance. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Triangular Carrier Frequency [Hz]"/>
                                                    <a:a name="UUID" value="ECUC:cb7e14da-da73-4b07-9487-c8f37d4a2abe"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="node:fallback(../PmicSpectrumModulationEnable, 'false') = 'true'"/>
                                                    </a:a>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.SpreadSpectrum.Frequencies.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.SpreadSpectrum.Frequencies.List')"/>
                                                </v:var>

                                                <v:var name="PmicFoutMuxSelection" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Configures which clock to be sent to the FOUT pin (FOUT_MUX_SEL[3:0]). <p/>
                                                                The FOUT pin can be used to synchronize an external device with the VR5510. <p/>
                                                                <strong>Warning:</strong> The function of the AMUX/FOUT pin is selected via the OTP settings. <p/>
                                                                This field is not editable unless the AMUX/FOUT pin is configured as FOUT
                                                                (PmicOtpMainUnitConfiguration/PmicOtpMainIOConfiguration/PmicAmuxFoutPinMode = 'FOUT_MODE'). <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FOUT Clock Mux Selection"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:9e8a476c-ef94-48e7-a9fa-cbd541bdab58"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpMainIOConfiguration/PmicAmuxFoutPinMode, 'AMUX_MODE') = 'FOUT_MODE'"/>
                                                    </a:a>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.FOUT.ClockMux.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.FOUT.ClockMux.List')"/>
                                                </v:var>

                                                <v:var name="PmicFoutClockSelection" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Configures the FOUT Source Selection (FOUT_CLK_SEL). <p/>
                                                                This field is editable only when PmicFoutMuxSelection = FOUT_CLK <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FOUT Source Selection"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:9e8a476c-ef94-48e7-a9fa-cbd563badb58"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="node:fallback(../PmicFoutMuxSelection, ecu:get('PMIC.FOUT.ClockMux.Default')) = 'FOUT_CLK'"/>
                                                    </a:a>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.FOUT.ClockSource.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.FOUT.ClockSource.List')"/>
                                                </v:var>

                                                <v:var name="PmicFoutPhaseDelay" type="INTEGER">
                                                  <a:a name="DESC">
                                                      <a:v>
                                                          <![CDATA[EN:
                                                              <html>
                                                              Controls the phase delay of the FOUT_CLK (FOUT_PHASE[2:0]). <p/>
                                                              This field is editable only when PmicFoutMuxSelection = FOUT_CLK <p/>
                                                              Note: Implementation Specific Parameter. <p/>
                                                              </html>
                                                          ]]>
                                                      </a:v>
                                                  </a:a>
                                                  <a:a name="LABEL" value="FOUT Phase Delay (cycles)" />
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-c2b4cb12ea7a"/>
                                                  <a:a name="EDITABLE" type="XPath">
                                                      <a:tst expr="node:fallback(../PmicFoutMuxSelection, ecu:get('PMIC.FOUT.ClockMux.Default')) = 'FOUT_CLK'"/>
                                                  </a:a>
                                                  <a:da name="DEFAULT" type="XPath" expr="num:i(0)"/>
                                                  <a:da name="INVALID" type="Range">
                                                      <a:tst expr="&lt;=7"/>
                                                      <a:tst expr="&gt;=0"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="PmicClk1Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                CLK1 Reference Frequency [Hz]. <p/>
                                                                Note: This field must not be manually modified. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="CLK1 Frequency [Hz]"/>
                                                    <a:a name="UUID" value="ECUC:72954dc6-f1aa-4644-92ca-ad57456489ea"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&gt;=1777777"/>
                                                        <a:tst expr="&lt;=2666666"/>
                                                    </a:da>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:f(
                                                                        round
                                                                        (
                                                                            num:f
                                                                            (
                                                                                num:f
                                                                                    (
                                                                                        (   num:f(number( node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicPllEnable, 'false') = 'false' )) *
                                                                                            num:f(text:replace(node:fallback(../PmicIrcoscFrequencyHz, 'F_20000000'),'F_','')) +
                                                                                            num:f(number( node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicPllEnable, 'false') = 'true' )) *
                                                                                            (
                                                                                                num:f(number( node:fallback(../PmicPllClockSelection, 'IRCOSC_CLK') = 'IRCOSC_CLK' )) *
                                                                                                num:f(text:replace(node:fallback(../PmicIrcoscFrequencyHz, 'F_20000000'),'F_','')) +
                                                                                                num:f(number( node:fallback(../PmicPllClockSelection, 'IRCOSC_CLK') = 'FIN_CLK' )) *
                                                                                                num:f(node:fallback(../PmicExternalPIN_Fin_FrequencyHz, 500000)) div
                                                                                                num:f(node:when(num:f(node:fallback(../PmicExternalPIN_Fin_FrequencyHz, 500000)) &gt;= num:f(2000000), 6, 1)) *
                                                                                                num:f(48)
                                                                                            )
                                                                                        )
                                                                                    ) div num:f(text:replace(node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicClk1Divider, 9),'DIV_',''))
                                                                            ) div num:f(1000)
                                                                        ) * num:f(1000)
                                                                    )"/>
                                                    </a:da>
                                                    <a:da name="RANGE" type="XPath">
                                                        <a:tst expr="
                                                                (
                                                                    not( node:empty( node:fallback(., 800000000) ) )
                                                                    and
                                                                    (
                                                                        num:f( node:fallback(., 800000000) ) !=
                                                                        (
                                                                            num:f(
                                                                                round
                                                                                (
                                                                                    num:f
                                                                                    (
                                                                                        num:f
                                                                                            (
                                                                                                (   num:f(number( node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicPllEnable, 'false') = 'false' )) *
                                                                                                    num:f(text:replace(node:fallback(../PmicIrcoscFrequencyHz, 'F_20000000'),'F_','')) +
                                                                                                    num:f(number( node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicPllEnable, 'false') = 'true' )) *
                                                                                                    (
                                                                                                        num:f(number( node:fallback(../PmicPllClockSelection, 'IRCOSC_CLK') = 'IRCOSC_CLK' )) *
                                                                                                        num:f(text:replace(node:fallback(../PmicIrcoscFrequencyHz, 'F_20000000'),'F_','')) +
                                                                                                        num:f(number( node:fallback(../PmicPllClockSelection, 'IRCOSC_CLK') = 'FIN_CLK' )) *
                                                                                                        num:f(node:fallback(../PmicExternalPIN_Fin_FrequencyHz, 500000)) div
                                                                                                        num:f(node:when(num:f(node:fallback(../PmicExternalPIN_Fin_FrequencyHz, 500000)) &gt;= num:f(2000000), 6, 1)) *
                                                                                                        num:f(48)
                                                                                                    )
                                                                                                )
                                                                                            ) div num:f(text:replace(node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicClk1Divider, 9),'DIV_',''))
                                                                                    ) div num:f(1000)
                                                                                ) * num:f(1000)
                                                                            )
                                                                        )
                                                                    )
                                                                )"
                                                            true="Please press the button to calculate the value automatically."/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="PmicClk2Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                CLK2 Reference Frequency [Hz]. <p/>
                                                                Note: This field must not be manually modified. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="CLK2 Frequency [Hz]"/>
                                                    <a:a name="UUID" value="ECUC:72954dc6-f1aa-4644-92ca-ad75236489ea"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&gt;=363636"/>
                                                        <a:tst expr="&lt;=545454"/>
                                                    </a:da>
