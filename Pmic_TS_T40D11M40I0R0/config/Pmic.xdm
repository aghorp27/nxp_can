                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.OutputVoltage.List')"/>
                                                              <a:da name="INVALID" type="XPath">
                                                                  <a:tst expr="
                                                                        (
                                                                            (num:f(text:replace(text:replace(node:fallback(., ecu:get('PMIC.OTP.VPRE.OutputVoltage.Default')),'V', '.'), 'OUT_', '')) &lt;= num:f(3.9)) and (node:fallback(../../../PmicOtpPowerSequenceConfiguration/PmicBoostRegulator/PmicBoostEnableRegulator, 'true') = 'false')
                                                                        )"
                                                                        true="When the BOOST regulator is disabled, the output voltage of VPRE must be set higher than 3.9V" />
                                                              </a:da>
                                                            </v:var>

                                                            <v:var name="PmicVpreSlopeCompensation" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the slope compensation of the VPRE (HVBUCK) regulator (VPRESC_OTP[5:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VPRE Slope Compensation (mV/us)" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-c2b6ad862c2a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.VPRE.SlopeCompensation.Default')"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.SlopeCompensation.List')"/>
                                                            </v:var>

                                                            <v:var name="PmicVpreStdbyVoltageControlEnable" type="BOOLEAN">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          This field is used to enable/disable the 3V output voltage selection for VPRE in Standby Mode (VPREV_STDY_EN_OTP). <p/>
                                                                            0 - The output voltage of VPRE in Standby Mode is controlled by VPREV_OTP[5:0]
                                                                            (i.e. 'PmicVpreOutputVoltage'). <p/>
                                                                            1 - The output voltage of VPRE in Standby Mode is controlled according to the M_REG_CTRL3[VPREV_STBY] selection
                                                                            (i.e. 'PmicModeSettingConf/PmicRegulatorsConfiguration/PmicVpreRegulator/PmicVpreStandbyOutputVoltage'). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VPRE Standby Voltage Control Enable" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-1da3df253e7a"/>
                                                              <a:da name="DEFAULT" value="true"/>
                                                            </v:var>

                                                            <v:var name="PmicVpreCsaThreshold" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the current sense amplifier peak detection threshold of the VPRE (HVBUCK) regulator (VPREILIM_OTP[1:0]). <p/>
                                                                          Warning: The 150mV setting is not available for 2.22MHz. (VPRE Clock Select must be set to CLK2). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VPRE ILIM Sense Voltage" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-d3a6ad835e2a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.VPRE.CsaThreshold.Default')"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.CsaThreshold.List')"/>
                                                                <a:da name="INVALID" type="XPath">
                                                                <a:tst expr="(node:fallback(., ecu:get('PMIC.OTP.VPRE.CsaThreshold.Default')) = 'MAX_150_MV') and
                                                                             (node:fallback(../../../PmicOtpPowerSequenceConfiguration/PmicVpreRegulator/PmicVpreClockSelect, ecu:get('PMIC.FOUT.ClockSource.List')) = 'CLK1')"
                                                                       true="The 150mV setting is not available for a clock source selection of 2.22MHz (CLK1)."/>
                                                              </a:da>
                                                            </v:var>

                                                            <v:var name="PmicVpreHsCurrentCapability" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the pull-up current capability of the HS (High-Side) gate driver of the VPRE (HVBUCK) regulator (VPRESRHS_OTP[1:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VPRE HS Gate Driver Pull-Up Capability" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-c2b2bc515e7a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.GateDriverCurrentCapability.List')[num:i(1)]"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.GateDriverCurrentCapability.List')"/>
                                                            </v:var>

                                                            <v:var name="PmicVpreLsCurrentCapability" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the pull-down current capability of the LS (Low-Side) gate driver of the VPRE (HVBUCK) regulator (VPRESRLS_OTP[1:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VPRE LS Gate Driver Pull-Down Capability" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-c2b6ad835e7a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.GateDriverCurrentCapability.List')[num:i(1)]"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.GateDriverCurrentCapability.List')"/>
                                                            </v:var>

                                                            <v:var name="PmicVpreSoftStartRamp" type="INTEGER">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the soft start ramp (from 0V to 1V) of the VPRE (HVBUCK) regulator (VPRE_SSRAMP_OTP). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VPRE Soft Start Ramp (mV/us)" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-d3a6a5d21e2a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="num:i('2')"/>
                                                              <a:da name="INVALID" type="Range">
                                                                  <a:tst expr="&lt;=2"/>
                                                                  <a:tst expr="&gt;=1"/>
                                                              </a:da>
                                                            </v:var>

                                                            <v:var name="PmicVpreHsOffTime" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the minimum OFF time of the HS (High-Side) gate driver of the VPRE (HVBUCK) regulator (VPRETOFF_OTP[1:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VPRE HS Minimum OFF Time" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-c2b3ca235e7a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.HSOffTime.List')[num:i(1)]"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.HSOffTime.List')"/>
                                                            </v:var>

                                                            <v:var name="PmicVpreHsOnTime" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the minimum ON time of the HS (High-Side) gate driver of the VPRE (HVBUCK) regulator (VPRE_PFM_TON_OTP[1:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VPRE HS Minimum ON Time" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-c2b3ca142e4a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.VPRE.HSOnTime.Default')"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.HSOnTime.List')"/>
                                                            </v:var>

                                                            <v:var name="PmicVpreShutdownDelay" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the shutdown delay between VBOOST and VPRE (VPRE_OFF_DLY_OTP). <p/>
                                                                          When shutting down, the device follows the power down sequence to stop all the regulators in the reverse
                                                                          order of the power up sequence. The value of this field is an additional delay added to the T<sub>slot</sub>
                                                                          time (PmicSlotWidth) between "SLOT_0" and "VPRE_OFF". <p/>
                                                                          To be used in case VPRE is supplying an external PMIC to wait its power down sequence completion. <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VPRE Shutdown Delay" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-d2a2ca532e4a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.VPRE.OffDelay.Default')"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VPRE.OffDelay.List')"/>
                                                            </v:var>
                                                        </v:ctr>

                                                        <v:ctr name="PmicVBoostRegulator" type="IDENTIFIABLE">
                                                            <a:a name="DESC">
                                                                <a:v>
                                                                    <![CDATA[EN:
                                                                        <html>
                                                                        This container contains the configuration parameters of the VBOOST regulator. <p/>
                                                                        Note: Implementation Specific Parameter. <p/>
                                                                        </html>
                                                                    ]]>
                                                                </a:v>
                                                            </a:a>
                                                            <a:a name="UUID" value="ECUC:9f1209fb-5482-4e6b-bf29-2341cb19da2d"/>

                                                            <v:var name="PmicVBoostOutputVoltage" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the output voltage of the VBOOST regulator (VBSTV_OTP[3:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VBOOST Output Voltage" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-c1a5ad32ea7a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.VBOOST.OutputVoltage.Default')"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VBOOST.OutputVoltage.List')"/>
                                                            </v:var>

                                                            <v:var name="PmicVBoostSlopeCompensation" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the slope compensation of the VBOOST regulator (VBSTSC_OTP[4:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VBOOST Slope Compensation (mV/us)" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-c2b6da346a2a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.VBOOST.SlopeCompensation.Default')"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VBOOST.SlopeCompensation.List')"/>
                                                              <a:da name="WARNING" type="XPath">
                                                                  <a:tst expr="
                                                                        (
                                                                            (node:fallback(../PmicVBoostCurrentLimit, ecu:get('PMIC.OTP.VBOOST.CurrentLimit.Default')) = 'I_2_A') and
                                                                            (node:fallback(., ecu:get('PMIC.OTP.VBOOST.SlopeCompensation.Default')) != 'SLOPE_160')
                                                                        )"
                                                                        true="It is highly recommended to set the slope compensation to 160mV/us when the output current (PmicVBoostCurrentLimit) is limited to 2A."/>
                                                                  <a:tst expr="
                                                                        (
                                                                            (node:fallback(../PmicVBoostCurrentLimit, ecu:get('PMIC.OTP.VBOOST.CurrentLimit.Default')) = 'I_3_A') and
                                                                            (node:fallback(., ecu:get('PMIC.OTP.VBOOST.SlopeCompensation.Default')) != 'SLOPE_67')
                                                                        )"
                                                                        true="It is highly recommended to set the slope compensation to 67mV/us when the output current (PmicVBoostCurrentLimit) is limited to 3A."/>
                                                              </a:da>
                                                            </v:var>

                                                            <v:var name="PmicVBoostLsOnTime" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the minimum ON time of the LS (Low-Side) gate driver of the VBOOST regulator (VBSTTONTIME_OTP[1:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VBOOST LS Minimum ON Time" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-c3a2cf142e4a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.VBOOST.LSOnTime.Default')"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VBOOST.LSOnTime.List')"/>
                                                            </v:var>

                                                            <v:var name="PmicVBoostCurrentLimit" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls (limits) the output current of the VBOOST regulator (VBSTILIM_OTP[1:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VBOOST Current Limit" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-d3a6da528e2a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.VBOOST.CurrentLimit.Default')"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VBOOST.CurrentLimit.List')"/>
                                                            </v:var>

                                                            <v:var name="PmicVBoostSlewRate" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the slew rate of the VBOOST regulator (VBSTSR_OTP[1:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
                                                                      ]]>
                                                                  </a:v>
                                                              </a:a>
                                                              <a:a name="LABEL" value="VBOOST Slew Rate Control (V/us)" />
                                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              </a:a>
                                                              <a:a name="ORIGIN" value="NXP"/>
                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-e1b5ad346a2a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.VBOOST.SlewRate.Default')"/>
                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.VBOOST.SlewRate.List')"/>
                                                              <a:da name="WARNING" type="XPath">
                                                                <a:tst expr="
                                                                        (
                                                                            (
                                                                                (node:fallback(../PmicVBoostCurrentLimit, ecu:get('PMIC.OTP.VBOOST.CurrentLimit.Default')) = 'I_2_A') or
                                                                                (node:fallback(../PmicVBoostCurrentLimit, ecu:get('PMIC.OTP.VBOOST.CurrentLimit.Default')) = 'I_3_A')
                                                                            ) and
                                                                            (node:fallback(., ecu:get('PMIC.OTP.VBOOST.SlewRate.Default')) != 'RATE_500')
                                                                        )"
                                                                        true="It is highly recommended to set the slew rate to 500V/us when the output current (PmicVBoostCurrentLimit) is limited to 2A or 3A."/>
                                                              </a:da>
                                                            </v:var>

                                                            <v:var name="PmicVBoostCompensationCapacitor" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
