                            <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                                <ad:ADMIN-DATA>
                                    <ad:LANGUAGE>EN</ad:LANGUAGE>
                                    <ad:DOC-REVISIONS>
                                        <ad:DOC-REVISION>
                                            <ad:REVISION-LABEL>4.0.0</ad:REVISION-LABEL>
                                            <ad:ISSUED-BY>NXP</ad:ISSUED-BY>
                                            <ad:DATE>2010-12-03</ad:DATE>
                                        </ad:DOC-REVISION>
                                    </ad:DOC-REVISIONS>
                                </ad:ADMIN-DATA>
                            </a:a>
                            <a:a name="POSTBUILDVARIANTSUPPORT" value="true"/>
                            <a:a name="UUID" value="ECUC:b510328c-ff12-4d58-bbbc-a5e7c2db3e2b"/>

                            <v:var name="POST_BUILD_VARIANT_USED" type="BOOLEAN">
                                <a:a name="DESC"
                                     value="Indicates whether a module implementation has or plans to have (i.e., introduced at link or post-build time) new post-build variation points."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="LABEL" value="Post Build Variant Used"/>
                                <a:a name="ORIGIN" value="EB"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:da name="DEFAULT" value="false"/>
                                <a:da name="READONLY" value="FALSE"/>
                                <a:da name="TOOLTIP"
                                    value="Indicates whether a module implementation has or plans to have (i.e., introduced at link or post-build time) new post-build variation points."/>
                            </v:var>

                            <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                                <a:a name="LABEL" value="Config Variant"/>
                                <a:a name="UUID" value="ECUC:e2d81221-d902-4f85-bd93-578fada96969"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:<html>
                                            VariantPreCompile: Only precompile time configuration parameters. Only one set of parameters.<p/>
                                            VariantPostBuild: Mix of precompile and postbuild time configuration parameters. Only one set of parameters.<p/>
                                            VariantPostBuildSelectable: Mix of precompile and postbuild time configuration parameters. More sets of parameters.<p/>
                                        </html>]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:da name="DEFAULT" value="VariantPostBuild"/>
                                <a:da name="RANGE">
                                    <a:v>VariantPostBuild</a:v>
                                    <a:v>VariantPreCompile</a:v>
                                </a:da>
                            </v:var>

                            <!-- /** @implements PmicGeneralConfiguration_Object */ -->
                            <v:ctr name="PmicGeneralConfiguration" type="IDENTIFIABLE">
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:<html>
                                         <p>   This container contains the global configuration parameters of the Non-Autosar PMIC driver.</p>
                                         <p>   Note: Implementation Specific Parameter. <p/>
                                        </html>]]>
                                    </a:v>
                                </a:a>
                                <a:a name="UUID" value="ECUC:b91249b0-4a69-496c-86b9-4cf3ec281e92"/>

                                <!-- /** @implements PmicDevErrorDetect_Object */ -->
                                <v:var name="PmicDevErrorDetect" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Pre-processor switch for enabling the default error detection and reporting to the DET. <p/>
                                                This field shall switch the Default Error Tracer (DET) detection and notification ON or OFF. <p/>
                                                The detection of default errors is configurable (ON/OFF) at precompile time. <p/>
                                                #define PMIC_DEV_ERROR_DETECT (STD_ON)/(STD_OFF) will be generated in Pmic_Cfg.h file. <p/>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="LABEL" value="PMIC Default Error Detection"/>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:564e05d8-6969-4a7d-b112-ebfa5e67d2d8"/>
                                    <a:da name="DEFAULT" value="true"/>
                                </v:var>

                                <!-- /** @implements PmicDisableDemReportErrorStatus_Object */ -->
                                <v:var name="PmicDisableDemReportErrorStatus" type="BOOLEAN">
                                    <a:a name="LABEL" value="Pmic Disable Production Error Reporting"/>
                                    <a:a name="DESC">
                                    <a:v>
                                      <![CDATA[EN:
                                          <html>
                                                Enable/Disable the API for reporting the Dem Error. <p/>
                                                #define PMIC_DISABLE_DEM_REPORT_ERROR_STATUS (STD_OFF)/(STD_ON) will be generated in Pmic_Cfg.h file. <p/>
                                                STD_ON will be applied if PmicDisableDemReportErrorStatus is true or PmicDemEventParameterRefs is not enabled.
                                                Note: Implementation Specific Parameter. <p/>
                                          </html>
                                      ]]>
                                   </a:v>
                                  </a:a>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                  </a:a>
                                 <a:a name="ORIGIN" value="NXP"/>
                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                  <a:a name="SCOPE" value="LOCAL"/>
                                  <a:a name="UUID" value="ECUC:72a8a638-4009-4639-124a-bb4daf86333b"/>
                                  <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <v:var name="PmicTimeoutMechanism" type="ENUMERATION">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                This parameter is used to select between two different timeout mechanisms. <p/>
                                                When PmicTimeoutMechanism = LOOPS, the driver will use loop counting for timeouts
                                                and the maximum number of iterations will be given by the PmicTimeoutDuration parameter. <p/>
                                                #define PMIC_TIMEOUT_MECHANISM_LOOPS (STD_OFF)/(STD_ON) will be generated in Pmic_Cfg.h file. <p/>
                                                When PmicTimeoutMechanism = TICKS, the driver will use the Time Service module for deterministic
                                                timeouts and the maximum number of microseconds will be given by the PmicTimeoutDuration parameter. <p/>
                                                Internally, it is using the 1us32bit set of API services. </p>
                                                #define PMIC_TIMEOUT_MECHANISM_TICKS (STD_OFF)/(STD_ON) will be generated in Pmic_Cfg.h file. <p/>
                                                Note: The latter choice generates an additional dependency. <p/>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:3b8b947e-d4c1-425a-b785-c8d125b7255f"/>
                                    <a:da name="DEFAULT" value="LOOPS"/>
                                    <a:a name="LABEL" value="Pmic Timeout Mechanism"/>
                                    <a:da name="RANGE">
                                        <a:v>LOOPS</a:v>
                                        <a:v>TICKS</a:v>
                                    </a:da>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(
                                                         (node:fallback(., 'LOOPS') = 'TICKS') and
                                                         (not(node:exists(as:modconf('Base')[1])))
                                                     )"
                                               true ="To use TICKS timeout, Base module should be included as dependency"
                                        />
                                    </a:da>
                                </v:var>

                                <v:var name="PmicTimeoutMethod" type="ENUMERATION">
                                    <a:a name="LABEL" value="Pmic Timeout Method"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:
                                                <html><p>
                                                    Vendor specific: Counter type used in timeout .<br>
                                                    Based on selected counter type the timeout value will be interpreted as follows:<br>
                                                    OSIF_COUNTER_SYSTEM - Use system counter.<br>
                                                    OSIF_COUNTER_CUSTOM - Use custom counter.<br>
                                                    OSIF_COUNTER_DUMMY  - Use dummy counter.<br>
                                                </p></html>
                                            ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:2b1fef6b-4321-1fa8-b2db-0ac159b82c69"/>
                                    <a:da name="DEFAULT" value="OSIF_COUNTER_SYSTEM"/>
                                    <a:da name="RANGE">
                                        <a:v>OSIF_COUNTER_SYSTEM</a:v>
                                        <a:v>OSIF_COUNTER_CUSTOM</a:v>
                                        <a:v>OSIF_COUNTER_DUMMY</a:v>
                                    </a:da>
                                    <a:a name="EDITABLE" type="XPath" expr="(node:fallback(../PmicTimeoutMechanism, 'LOOPS') = 'TICKS')"/>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(
                                                        (node:fallback(../PmicTimeoutMechanism, 'LOOPS') = 'TICKS') and
                                                        (node:fallback(., 'OSIF_COUNTER_SYSTEM') = 'OSIF_COUNTER_SYSTEM') and
                                                        (node:fallback(as:modconf('Base')[1]/OsIfGeneral/OsIfUseSystemTimer, 'false') != 'true')
                                                     )"
                                               true="If PmicTimeoutMethod is OSIF_COUNTER_SYSTEM, Base/OsIfGeneral/OsIfUseSystemTimer should be true"
                                        />
                                        <a:tst expr="(
                                                        (node:fallback(../PmicTimeoutMechanism, 'LOOPS') = 'TICKS') and
                                                        (node:fallback(., 'OSIF_COUNTER_SYSTEM') = 'OSIF_COUNTER_CUSTOM') and
                                                        (node:fallback(as:modconf('Base')[1]/OsIfGeneral/OsIfUseCustomTimer, 'false') != 'true')
                                                     )"
                                               true="If PmicTimeoutMethod is OSIF_COUNTER_CUSTOM, Base/OsIfGeneral/OsIfUseCustomTimer should be true"
                                        />
                                    </a:da>
                                </v:var>

                                <v:var name="PmicTimeoutDuration" type="INTEGER">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                When PmicTimeoutMechanism = LOOPS, the unit of measurement is given in number of iterations. <p/>
                                                When PmicTimeoutMechanism = TICKS, the unit of measurement is given in number of microseconds. <p/>
                                                Please take the above into consideration when choosing the value for this parameter. <p/>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:3b8b947e-d4c1-425a-b785-c8d140a8066f"/>
                                    <a:da name="DEFAULT" value="50000"/>
                                    <a:a name="LABEL" value="Pmic Timeout Duration"/>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&gt;=1"/>
                                        <a:tst expr="&lt;=4294967295"/>
                                    </a:da>
                                </v:var>

                                <!-- /** @implements PmicVersionInfoApi_Object */ -->
                                <v:var name="PmicVersionInfoApi" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Pre-processor switch to enable/disable the API to read out the modules version information (Pmic_GetVersionInfo). <p/>
                                                #define PMIC_VERSION_INFO_API (STD_ON)/(STD_OFF) will be generated in Pmic_Cfg.h file. <p/>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="LABEL" value="Provide PMIC Version Info API"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7ea93c38-3acb-4571-6868-c6c67d6746b1"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <!-- /** @implements PmicDeviceInfoApi_Object */ -->
                                <v:var name="PmicDeviceInfoApi" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Pre-processor switch to enable/disable the API to read out the device information (Pmic_GetDeviceInfo). <p/>
                                                #define PMIC_DEVICE_INFO_API (STD_ON)/(STD_OFF) will be generated in Pmic_Cfg.h file. <p/>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="LABEL" value="Provide PMIC Device Info API"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7ea93c38-3acb-4571-a374-c6d15e6742b1"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <!-- /** @implements PmicSetAnalogMuxApi_Object */ -->
                                <v:var name="PmicSetAnalogMuxApi" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Pre-processor switch to enable/disable the API to set the analog multiplexer (AMUX) pin. <p/>
                                                #define PMIC_SET_ANALOG_MUX_API (STD_ON)/(STD_OFF) will be generated in Pmic_Cfg.h file. <p/>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="LABEL" value="Provide Pmic_SetAnalogMux API"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7ea93c38-3acb-4571-a374-c6d41b1342b1"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(node:fallback(../../PmicGlobalConfig/PmicDevice/*/PmicAMUXConfiguration/PmicSetAnalogMuxApi,'false') != 'true')
                                        and (node:fallback(.,'false') = 'true')"
                                        true="If all devices disabled Amux API, the 'Provide Pmic_SetAnalogMux API' must be disabled."
                                        />
                                    </a:da>
                                </v:var>

                                <!-- /** @implements PmicWatchdogApi_Object */ -->
                                <v:var name="PmicWatchdogApi" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Pre-processor switch to enable/disable the Watchdog API. <p/>
                                                #define PMIC_WATCHDOG_API (STD_ON)/(STD_OFF) will be generated in CDD_Pmic_Cfg.h file. <p/>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="LABEL" value="Provide PMIC Watchdog API"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7ea93c38-3acb-4571-a374-c6c71e1342b1"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(node:fallback(../../PmicGlobalConfig/PmicDevice/*/PmicFailSafeConfiguration/PmicWatchdogConfiguration/PmicWatchdogApi,'false') != 'true')
                                        and (node:fallback(.,'false') = 'true')"
                                        true="If all devices disabled Watchdog API, the 'Provide Watchdog API' must be disabled."
                                        />
                                    </a:da>
                                </v:var>
                                <!-- /** @implements PmicOtpEmulationModeApi_Object */ -->
                                <v:var name="PmicOtpEmulationModeApi" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Pre-processor switch to enable/disable the OTP Emulation Mode API. <p/>
                                                #define PMIC_OTP_EMULATION_MODE_API (STD_ON)/(STD_OFF) will be generated in CDD_Pmic_Cfg.h file. <p/>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="LABEL" value="Provide Pmic_EmulateDeviceOTP API"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7ea93c38-3acb-4521-a374-c6c71e134777"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <!-- /** @implements PmicSwitchSVSApi_Object */ -->
                                <v:var name="PmicSwitchSVSApi" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                API shall re-configure static voltage scaler (SVS) after device was initialized. <p/>
                                                In case DIE_PROCESS feature is enable, this function will configure SVS if the DIE_PROCESS otp of mcu enables. <p/>
                                                It will not configure SVS if the DIE_PROCESS otp of mcu disable. Refer Mcu reference manual to check DIE_PROCESS bit. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="LABEL" value="Provide Pmic_SwitchSVS API"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7ea93c38-3acb-4521-a374-c6c71effe777"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(node:fallback(../../PmicGlobalConfig/PmicDevice/*/PmicFailSafeConfiguration/PmicSVSConfiguration/PmicSwitchSVSApi,'false') != 'true')
                                        and (node:fallback(.,'false') = 'true')"
                                        true="If all devices disabled switch SVS API, the 'Provide Pmic_SwitchSVS API' must be disabled."
                                        />
                                    </a:da>
                                </v:var>
                                <v:var name="PmicExternalWatchdog" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                External watchdog - true if external watchdog is used (Wdg_43_VR5510), all other watchdog settings in this driver will be ignored. </p>
                                                The driver will assume that the application is using an external watchdog driver instead of directly calling the 'Pmic_TriggerWatchdog' API. </p>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="LABEL" value="Use External Watchdog"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7ea93c38-ffff-afff-a374-c6c71e134777"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <v:var name="PmicWatchdogTaskNotification" type="FUNCTION-NAME">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Function name of callback. This function will be called when watchdog task is required and watchdog window is not DISABLE option. <p/>
                                                If this notification is enabled, the driver will assume that the application will service the external watchdog
                                                by directly calling the 'Pmic_TriggerWatchdog' API. </p>
                                                Note: The notification can only be enabled if the Watchdog API is provided. (i.e. PmicGeneralConfiguration/PmicWatchdogApi = 'true'). </p>
                                                Note: Implementation Specific Parameter.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="UUID" value="ECUC:8c7cba30-17c7-48bd-ae5f-9aeb0171b514"/>
                                    <a:a name="OPTIONAL" value="true"/>
                                    <a:da name="DEFAULT" value="NULL_PTR"/>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(text:toupper(normalize-space(node:fallback(., ''))) = '&quot;NULL_PTR&quot;') or
                                                     (text:toupper(normalize-space(node:fallback(., ''))) = 'NULL_PTR') or
                                                     (text:toupper(normalize-space(node:fallback(., ''))) = '&quot;NULL&quot;') or
                                                     (text:toupper(normalize-space(node:fallback(., ''))) = 'NULL') or not
                                                     (text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$'))"
                                           true="Invalid identifier. The field must specify a valid C function identifier (i.e. an arbitrarily long sequence of digits, underscores, lowercase and uppercase Latin letters, that must begin with a non-digit character)"/>
                                        <a:tst expr="node:exists(.) = 'true' and (node:fallback(../PmicWatchdogApi, 'false') = 'false')"
                                               true="To enable the watchdog task notification, the 'Provide Watchdog API' (PmicGeneralConfiguration/PmicWatchdogApi) checkbox must be enabled." />
                                        <a:tst expr="node:exists(.) = 'true' and (node:fallback(../PmicExternalWatchdog, 'true') = 'true')"
                                               true="To enable the watchdog task notification, the 'Use External Watchdog' (PmicGeneralConfiguration/PmicExternalWatchdog) checkbox must be disabled." />
                                    </a:da>
                                </v:var>
                                <v:var name="PmicDieProcessEnable" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                This node allow to enable/disable the checking DIE_PROCESS of MCU before setting static voltage scaler (SVS). </p>
                                                #define PMIC_DIE_PROCESS_ENABLE (STD_ON)/(STD_OFF) will be generated in Pmic_Cfg.h file. <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="LABEL" value="Support DIE_PROCESS"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7ea93c38-ffff-adff-a374-c6c71e134777"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:a name="VISIBLE" type="XPath">
                                        <a:tst expr="(text:tolower(text:contains(ecu:list('PMIC.DIE_PROCESS.SUPPORT'),'true'))='true')"/>
                                    </a:a>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="node:fallback(.,'true') = 'false' or (node:fallback(.,'true') = 'true') and node:refvalid(../PmicOcotplink)" false="Invalid OcotpChannel reference (Suggestion: select a OcotpChannel reference). Please add OCOTP module and select OcotpChannel."/>
                                    </a:da>
                                </v:var>

                                <v:ref name="PmicOcotplink" type="SYMBOLIC-NAME-REFERENCE">
                                    <a:a name="LABEL" value="Ocotp Channel Reference" />
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Reference to the Ocotp channel configuration used to check the die_process of MCU. <p/>
                                                Ocotp Channel must be configured the eFuse word as OCOTP_BANK0_WORD6 <p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
