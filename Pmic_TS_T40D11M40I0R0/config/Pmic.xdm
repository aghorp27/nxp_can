                                                              <a:a name="SCOPE" value="LOCAL"/>
                                                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                              <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-b1d5ab516e2a"/>
                                                              <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.OTP.BUCK1.LSelect.Default')"/>
