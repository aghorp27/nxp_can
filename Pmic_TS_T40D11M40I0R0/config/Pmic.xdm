                                            <!-- <a:da name="DEFAULT" value="0x20"/>
                                            <a:da name="RANGE" type="XPath" expr="node:foreach(text:range( num:i(ecu:get('PMIC.OTP.MU.I2cAddress.Low')), num:i(ecu:get('PMIC.OTP.MU.I2cAddress.High')), num:i(ecu:get('PMIC.OTP.MU.I2cAddress.Step')) ), 'val', 'num:inttohex(num:i($val))')"/> -->
                                            <a:da name="INVALID" type="XPath">
