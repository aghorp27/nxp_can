                                                              <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.OTP.BUCK1.CurrentLimit.List')"/>
                                                            </v:var>

                                                            <v:var name="PmicBuck1InductorSelect" type="ENUMERATION">
                                                              <a:a name="DESC">
                                                                  <a:v>
                                                                      <![CDATA[EN:
                                                                          <html>
                                                                          Controls the inductance (L<sub>Buck1</sub>) for V<sub>Buck1_SW</sub> of the BUCK1 regulator (BUCK1_LSELECT_OTP[1:0]). <p/>
                                                                          Note: Implementation Specific Parameter. <p/>
                                                                          </html>
