                                                                                                        (num:f(node:fallback(../../../PmicClk2Frequency, 455000))) +
                                                                                                        (num:f(number( node:fallback(../../../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpPowerSequenceConfiguration/PmicBuck2Regulator/PmicBuck2ClockSelect, 'CLK2') = 'CLK1'))) *
                                                                                                        (num:f(node:fallback(../../../PmicClk1Frequency, 2222000)))
                                                                                                    ) +
                                                                                                    (num:f(number( node:fallback(../../../PmicFoutMuxSelection, 'DISABLED') = 'BUCK3_CLK'))) *
                                                                                                    (
                                                                                                        (num:f(number( node:fallback(../../../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpPowerSequenceConfiguration/PmicBuck3Regulator/PmicBuck3ClockSelect, 'CLK2') = 'CLK2'))) *
                                                                                                        (num:f(node:fallback(../../../PmicClk2Frequency, 455000))) +
                                                                                                        (num:f(number( node:fallback(../../../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpPowerSequenceConfiguration/PmicBuck3Regulator/PmicBuck3ClockSelect, 'CLK2') = 'CLK1'))) *
                                                                                                        (num:f(node:fallback(../../../PmicClk1Frequency, 2222000)))
                                                                                                    ) +
                                                                                                    (num:f(number( node:fallback(../../../PmicFoutMuxSelection, 'DISABLED') = 'FOUT_CLK'))) *
                                                                                                    (
                                                                                                        (num:f(number( node:fallback(../../../PmicFoutClockSelection, 'CLK2') = 'CLK2'))) *
                                                                                                        (num:f(node:fallback(../../../PmicClk2Frequency, 455000))) +
                                                                                                        (num:f(number( node:fallback(../../../PmicFoutClockSelection, 'CLK2') = 'CLK1'))) *
                                                                                                        (num:f(node:fallback(../../../PmicClk1Frequency, 2222000)))
                                                                                                    ) +
                                                                                                    (num:f(number( node:fallback(../../../PmicFoutMuxSelection, 'DISABLED') = 'CLK_FIN_DIV'))) *
                                                                                                    (
                                                                                                        num:f(number( node:fallback(../../../PmicFinEnable, 'false') = 'true')) *
                                                                                                        num:f(node:fallback(../../../PmicExternalPIN_Fin_FrequencyHz, 500000)) div
                                                                                                        num:f(node:when(num:f(node:fallback(../../../PmicExternalPIN_Fin_FrequencyHz, 500000)) &gt;= num:f(2000000), 6, 1))
                                                                                                    ) +
                                                                                                    (num:f(number( node:fallback(../../../PmicFoutMuxSelection, 'DISABLED') = 'CLK20M_MAIN_DIV48'))) *
                                                                                                    (
                                                                                                        (num:f(text:replace(node:fallback(../../../PmicIrcoscFrequencyHz, 'F_20000000'),'F_',''))) div
                                                                                                        (num:f(48))
                                                                                                    ) +
                                                                                                    (num:f(number( node:fallback(../../../PmicFoutMuxSelection, 'DISABLED') = 'CLK20M_FS_DIV48'))) *
                                                                                                    (
                                                                                                        (num:f(20000000) div num:f(48))
                                                                                                    )
                                                                                                )
                                                                                            )
                                                                                        )
                                                                                )"
                                                                true="Please press the button to calculate the value automatically."/>
                                                            </a:a>
                                                            <a:da name="INVALID" type="XPath">
                                                                <a:tst expr="(  (node:fallback(../PmicClockFrequencySelect, 'VPRE_CLK') = 'VPRE_CLK') and
                                                                                (
                                                                                    (
                                                                                        ( num:i(node:fallback(., 455000)) &lt; num:i(430000) ) or
                                                                                        (
                                                                                            ( num:i(node:fallback(., 455000)) &gt; num:i(480000) ) and
                                                                                            ( num:i(node:fallback(., 455000)) &lt; num:i(2100000) )
                                                                                        ) or
                                                                                        ( num:i(node:fallback(., 455000)) &gt; num:i(2350000) )
                                                                                    )
                                                                                )
                                                                             )"
                                                                    true="Error: VPRE_CLK shall have a switching frequency range of either between 452KHz and 457KHz (with a typical frequency of 455KHz) or between 2.21MHz and 2.35MHz (with a typical frequency of 2.22MHz)"/>
                                                                <a:tst expr="(  (node:fallback(../PmicClockFrequencySelect, 'VPRE_CLK') = 'BUCK1_CLK') and
                                                                                (
                                                                                    ( num:i(node:fallback(., 2222000)) &lt; num:i(2100000) ) or
                                                                                    ( num:i(node:fallback(., 2222000)) &gt; num:i(2350000) )
                                                                                )
                                                                             )"
                                                                    true="Error: BUCK1_CLK shall have a switching frequency range between 2.21MHz and 2.35MHz (with a typical frequency of 2.22MHz)"/>
                                                                <a:tst expr="(  (node:fallback(../PmicClockFrequencySelect, 'VPRE_CLK') = 'BUCK2_CLK') and
                                                                                (
                                                                                    ( num:i(node:fallback(., 2222000)) &lt; num:i(2100000) ) or
                                                                                    ( num:i(node:fallback(., 2222000)) &gt; num:i(2235000) )
                                                                                )
                                                                             )"
                                                                    true="Error: BUCK2_CLK shall have a switching frequency range between 2.21MHz and 2.35MHz (with a typical frequency of 2.22MHz)"/>
                                                                <a:tst expr="(  (node:fallback(../PmicClockFrequencySelect, 'VPRE_CLK') = 'BUCK3_CLK') and
                                                                                (
                                                                                    ( num:i(node:fallback(., 2222000)) &lt; num:i(2100000) ) or
                                                                                    ( num:i(node:fallback(., 2222000)) &gt; num:i(2350000) )
                                                                                )
                                                                             )"
                                                                    true="Error: BUCK3_CLK shall have a switching frequency range between 2.21MHz and 2.35MHz (with a typical frequency of 2.22MHz)"/>
                                                            </a:da>
                                                            <a:da name="INVALID" type="Range">
                                                                <a:tst expr="&lt;=3500000"/>
                                                                <a:tst expr="&gt;=0"/>
                                                            </a:da>
                                                        </v:var>

                                                    </v:ctr>
                                                </v:lst>

                                            </v:ctr>
                                        </v:lst>

                                        <!-- /** @implements PmicModeSettingConf_Object */ -->
                                        <v:lst name="PmicModeSettingConf" type="MAP">
                                            <a:da name="MIN" value="1"/>

                                            <v:ctr name="PmicModeSettingConf" type="IDENTIFIABLE">
                                                <a:a name="REQUIRES-INDEX" value="true"/>
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            This container contains the configuration for the Mode setting of the PMIC. <p/>
                                                            Note: Implementation Specific Parameter. <p/>
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="UUID" value="ECUC:9d338d97-6006-469a-8111-ab7a39ba66ec"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                                </a:a>

                                                <!-- /** @implements PmicModeID_Object */ -->
                                                <v:var name="PmicModeID" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                This parameter shall represent the ID of the PMIC mode. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="LABEL" value="Mode ID"/>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:f296ee65-d7f7-4f6d-ae0c-564823d649b4"/>
                                                    <a:da name="DEFAULT" type="XPath" expr="num:i(node:fallback(node:current()/../@index, num:i(0)))"/>
                                                    <a:da name="RANGE" type="XPath">
                                                        <a:tst expr="text:uniq(../../*/PmicModeID, .)"
                                                            false="Duplicated value: PmicModeID must be unique across PmicModeSettingConf. Use the 'Calculate  Value' button to calculate the correct default value."/>
                                                        <a:tst expr="(. &gt;= 0) and (. &lt; num:i(count(node:current()/../../*)))"
                                                            false="Value out of range: must be in range 0 to N-1 (N is number of configured mode settings). Use the 'Calculate Value' button to calculate the correct default value."/>
                                                        <a:tst expr="num:i(node:fallback(.,0)) = num:i(../@index)"
                                                            false="Invalid value: The ID should correspond to the position of the element within the configuration list "/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=255"/>
                                                        <a:tst expr="&gt;=0"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="PmicModeSelection" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                This parameter selects the Mode to be used. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="LABEL" value="Operating Mode"/>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:4e601bd3-e1bf-40ea-8318-741323d8615e"/>
                                                    <a:da name="DEFAULT" value="NORMAL"/>
                                                    <a:da name="RANGE">
                                                        <a:v>NORMAL</a:v>
                                                        <a:v>RESET</a:v>
                                                        <a:v>SHUTDOWN</a:v>
                                                        <a:v>STANDBY</a:v>
                                                        <a:v>DEEPSLEEP</a:v>
                                                    </a:da>
                                                    <a:a name="INVALID" type="XPath">
                                                        <a:tst expr="(node:fallback(., 'NORMAL') = 'DEEPSLEEP') and
                                                                     (node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicDeepSleepEnable, 'false') = 'false')"
                                                               true="DeepSleep Mode is disabled by OTP (i.e. PmicOtpMainUnitConfiguration/PmicOtpDeviceConfiguration/PmicDeepSleepEnable = 'false')."/>
                                                        <a:tst expr="(node:fallback(., 'NORMAL') = 'STANDBY') and
                                                                     (node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyIOConfiguration/PmicStandbyModeEnable, 'false') = 'false')"
                                                               true="Standby Mode is disabled by OTP (i.e. PmicOtpFailSafeUnitConfiguration/PmicOtpSafetyIOConfiguration/PmicStandbyModeEnable = 'false')."/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="PmicPWRON2DeepSleepModeEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                This field is used to enable/disable the entry to DeepSleep Mode by toggling the PWRON2 pin (M_MODE[PWRON2_DSM_EN]). <p/>
                                                                0 - PWRON2 (high-to-low) transition cannot trigger DSM <p/>
                                                                1 - PWRON2 (high-to-low) transition can trigger DSM <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="LABEL" value="PWRON2 Trigger DSM"/>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:524e05d8-8922-4a7d-b112-ead3fe25d2d8"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../PmicModeSelection, 'NORMAL') = 'DEEPSLEEP') or ((node:fallback(../PmicModeSelection, 'NORMAL') != 'DEEPSLEEP') and (node:fallback(., 'false') = 'true'))"/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="XPath">
                                                        <a:tst expr="(node:fallback(., 'false') = 'false') and (node:fallback(../PmicModeSelection, 'NORMAL') = 'DEEPSLEEP')"
                                                               true="To trigger DeepSleep Mode, this field must be set to 'true'."/>
                                                       <a:tst expr="(node:fallback(., 'false') = 'true') and (node:fallback(../PmicModeSelection, 'NORMAL') != 'DEEPSLEEP')"
                                                               true="If selected mode isn't DEEPSLEEP, PWRON2 Trigger DSM should be set to default."/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="PmicPWRON1WakeUpEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                This field is used to enable/disable the wake-up functionality of the PWRON1 pin (M_MODE[PWRON1DIS]). <p/>
                                                                0 - PWRON1 Wakeup Disable <p/>
                                                                1 - PWRON1 Wakeup Enable <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="LABEL" value="PWRON1 Wakeup Enable"/>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:524e05d8-8922-4a7d-b112-edaf3e20d2d8"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                    <a:a name="INVALID" type="XPath">
                                                        <a:tst expr="(node:fallback(., 'true') = 'false') and
                                                                     (node:fallback(../PmicModeSelection, 'NORMAL') = 'SHUTDOWN') and
                                                                     (node:fallback(../PmicPWRON2WakeUpEnable, 'true') = 'false')"
                                                               true="PWRON1 WakeUp or PWRON2 WakeUp must be enabled if PmicModeSelection is 'SHUTDOWN'"/>
                                                        <a:tst expr="(node:fallback(., 'true') = 'false') and
                                                                     (node:fallback(../PmicModeSelection, 'NORMAL') = 'DEEPSLEEP') and
                                                                     (node:fallback(../PmicPWRON2WakeUpEnable, 'true') = 'false')"
                                                               true="PWRON1 WakeUp or PWRON2 WakeUp must be enabled if PmicModeSelection is 'DEEPSLEEP'"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="PmicPWRON2WakeUpEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                        <![CDATA[EN:<html>
                                                            This field is used to enable/disable the wake-up functionality of the PWRON2 pin (M_MODE[PWRON2DIS]). <p/>
                                                            0 - PWRON2 Wakeup Disable <p/>
                                                            1 - PWRON2 Wakeup Enable <p/>
                                                            Note: Implementation Specific Parameter. <p/>
                                                        </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="LABEL" value="PWRON2 Wakeup Enable"/>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:524e05d8-8922-4a7d-b112-ebfa6250d1d8"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                    <a:a name="INVALID" type="XPath">
                                                        <a:tst expr="(node:fallback(., 'true') = 'false') and
                                                                     (node:fallback(../PmicModeSelection, 'NORMAL') = 'SHUTDOWN') and
                                                                     (node:fallback(../PmicPWRON1WakeUpEnable, 'true') = 'false')"
                                                               true="PWRON2 WakeUp or PWRON1 WakeUp must be enabled if PmicModeSelection is 'SHUTDOWN'"/>
                                                        <a:tst expr="(node:fallback(., 'true') = 'false') and
                                                                     (node:fallback(../PmicModeSelection, 'NORMAL') = 'DEEPSLEEP') and
                                                                     (node:fallback(../PmicPWRON1WakeUpEnable, 'true') = 'false')"
                                                               true="PWRON2 WakeUp or PWRON1 WakeUp must be enabled if PmicModeSelection is 'DEEPSLEEP'"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="PmicStandbyTimerEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                        <![CDATA[EN:<html>
                                                            This field is used to enable/disable the standby transition timer (M_SM_CTRL1[STBY_TIMER_EN]). <p/>
                                                            0 - Standby Transition Timer is Disabled <p/>
                                                            1 - Standby Transition Timer is Enabled <p/>
                                                            The Standby Timer is implemented in the Main-Unit logic to automatically transition
                                                            the device to Deep Fail-Safe (DFS) mode in case of timeout during Standby mode. <p/>
                                                            Note: Implementation Specific Parameter. <p/>
                                                        </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="LABEL" value="Standby Timer Enable"/>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:524e05d8-8922-4a7d-b112-ebfa1286d1d8"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../PmicModeSelection, 'NORMAL') = 'STANDBY')"/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="XPath">
                                                        <a:tst expr="(node:fallback(., 'false') = 'true') and
                                                                     (node:fallback(../../../PmicOtpConfiguration/*[1]/PmicOtpMainUnitConfiguration/PmicOtpMainIOConfiguration/PmicStandbyTimerEnable, 'false') = 'false')"
                                                               true="The Standby Timer is disabled by OTP (i.e. PmicOtpMainUnitConfiguration/PmicOtpMainIOConfiguration/PmicStandbyTimerEnable = 'false')."/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="PmicStandbyTimerWindowDuration" type="ENUMERATION">
                                                  <a:a name="DESC">
                                                      <a:v>
                                                          <![CDATA[EN:
                                                              <html>
                                                              Configures the standby timer window duration (M_SM_CTRL1[TIMER_STBY_WINDOW]). <p/>
                                                              Represents the amount of time to wait in Standby mode until a timeout is triggered,
                                                              after which the device will automatically transition to Deep Fail-Safe (DFS) mode. <p/>
                                                              This field is editable only when the standby timer is enabled
                                                              (i.e. PmicStandbyTimerEnable = 'true'). <p/>
                                                              Note: Implementation Specific Parameter. <p/>
                                                              </html>
                                                          ]]>
                                                      </a:v>
                                                  </a:a>
                                                  <a:a name="LABEL" value="Standby Timer Window Duration" />
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-e7a2bc31ea4b"/>
                                                  <a:da name="DEFAULT" type="XPath" expr="ecu:get('PMIC.StandbyTimer.WindowDuration.Default')"/>
                                                  <a:da name="RANGE" type="XPath" expr="ecu:list('PMIC.StandbyTimer.WindowDuration.List')"/>
                                                  <a:a name="EDITABLE" type="XPath">
                                                      <a:tst expr="(node:fallback(../PmicModeSelection, 'NORMAL') = 'STANDBY')"/>
                                                      <a:tst expr="(node:fallback(../PmicStandbyTimerEnable, 'false') = 'true')"/>
                                                  </a:a>
                                                </v:var>

                                                <v:ctr name="PmicRegulatorsConfiguration" type="IDENTIFIABLE">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                This container contains the configuration parameters for the registers controlling the functionality of regulators. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                                </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="TAB" value="PmicRegulatorsConfig"/>
                                                    <a:a name="UUID" value="ECUC:9f1209fb-5482-4e6b-bf29-2341bc58da2d"/>

                                                    <v:ctr name="PmicVpreRegulator" type="IDENTIFIABLE">
                                                        <a:a name="DESC">
                                                            <a:v>
                                                                <![CDATA[EN:
                                                                    <html>
                                                                    This container contains the configuration parameters of the VPRE (HVBUCK) regulator. <p/>
                                                                    Note: Implementation Specific Parameter. <p/>
                                                                    </html>
                                                                ]]>
                                                            </a:v>
                                                        </a:a>
                                                        <a:a name="UUID" value="ECUC:9f1209fb-5482-4e6b-bf29-5126df67da2d"/>

                                                        <v:var name="PmicVpreEnableRegulator" type="ENUMERATION">
                                                            <a:a name="DESC">
                                                                <a:v>
                                                                    <![CDATA[EN:
                                                                        <html>
                                                                        This field is used to control the power state the VPRE (HVBUCK) regulator (M_REG_CTRL1[VPREDIS/VPREEN]). <p/>
                                                                          NO_EFFECT - Keep previous power state of VPRE <p/>
                                                                          DISABLE   - VPRE is Disabled <p/>
                                                                          ENABLE    - VPRE is Enabled <p/>
                                                                        This field is modifiable only when PmicModeSelection = 'NORMAL'. <p/>
                                                                        Note: Implementation Specific Parameter. <p/>
                                                                        </html>
                                                                    ]]>
                                                                </a:v>
                                                            </a:a>
                                                            <a:a name="LABEL" value="VPRE Normal Mode Power State" />
                                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                              <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                            </a:a>
                                                            <a:a name="ORIGIN" value="NXP"/>
                                                            <a:a name="SCOPE" value="LOCAL"/>
                                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
